<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="PrismDemo1.Views.DialogDemo">


    <StackLayout VerticalOptions="CenterAndExpand" >


        <Button Text="Dialog Box" Command="{Binding DialogCommand}" TextColor="White" BackgroundColor="#3d44ba" />
        <Button Text="Go Back" Command="{Binding GoBackCommand}" TextColor="White" BackgroundColor="Aqua" />
        
    </StackLayout>
  
</ContentPage>