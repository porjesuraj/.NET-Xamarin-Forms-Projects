<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIDeviceFamily</key>
	<array>
		<integer>1</integer>
		<integer>2</integer>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>MinimumOSVersion</key>
	<string>10.0</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>XSAppIconAssets</key>
	<string>Assets.xcassets/AppIcon.appiconset</string>
	<key>CFBundleDisplayName</key>
	<string>XamlNavigation</string>
	<key>CFBundleName</key>
	<string>XamlNavigation</string>
	<key>CFBundleIdentifier</key>
	<string>com.companyname.appname</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>We need access for your location to improve app experience</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>We need access for your location to improve app experience</string>
	<key>NSCalendarsUsageDescription</key>
	<string>We need to access to the calender</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>We need access to bluetooth to be able to access permission</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>We need access for maps</string>
	<key>NSLocationUsageDescription</key>
	<string>we need access to maps</string>
	<key>UIRequiresPersistentWiFi</key>
	<true/>

	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSExceptionDomains</key>
		<dict>
			<key>yourdomain.com</key>
			<dict>
				<!--Include to allow subdomains-->
				<key>NSIncludesSubdomains</key>
				<true/>
				<!--Include to allow HTTP requests-->
				<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
				<true/>
				<!--Include to specify minimum TLS version-->
				<key>NSTemporaryExceptionMinimumTLSVersion</key>
				<string>TLSv1.1</string>
			</dict>
		</dict>
	</dict>
</dict>
</plist>
