             xmlns:vm="clr-namespace:CalculatorApp.ViewModels"
             x:Class="CalculatorApp.Views.MainPage">
    
    <ContentPage.Content>
        <StackLayout>
            <Button Command="{Binding DIPageCommand}" Text="Go To Dependency injection Demo Page"  BackgroundColor="ForestGreen" TextColor="White" />
